   "source": [
    "class Person:\n",
    "  def __init__(self, name, age):\n",
    "    self.name = name\n",
    "    self.age = age\n",
    "\n",
    "  def __str__(self):\n",
    "    return f\"{self.name}+'__'+({self.age})\"\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
